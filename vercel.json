{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_WALLETCONNECT_PROJECT_ID": "@walletconnect-project-id",
    "VITE_GEMINI_API_KEY": "@gemini-api-key",
    "VITE_QUIZ_CONTRACT_ADDRESS": "@quiz-contract-address",
    "VITE_NFT_STAKING_CONTRACT_ADDRESS": "@nft-staking-contract-address",
    "VITE_GAME_LIFECYCLE_CONTRACT_ADDRESS": "@game-lifecycle-contract-address"
  }
}
